    "import sympy as sp\n",
    "\n",
    "x = sp.symbols('x')\n",
    "\n",
    "P = x**4 - 6*x**3 + 11*x**2 - 6*x\n",
    "\n",
    "raices = sp.solve(P, x)\n",
    "\n",
    "print(\"Las raíces del polinomio son:\", raices)"
